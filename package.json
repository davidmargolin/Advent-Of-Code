{
  "name": "advent2021",
  "version": "1.0.0",
  "description": "Advent Of Code Solutions",
  "scripts": {
    "start": "ts-node",
    "all": "for day in `seq 1 25`; do if [ -d $day ]; then echo '\nDay' $day && yarn --silent start $day; fi done",
