{
  "name": "advent2021",
  "version": "1.0.0",
  "description": "Advent Of Code Solutions",
  "scripts": {
    "start": "babel-node",
    "all": "for day in `seq 1 25`; do if [ -d $day ]; then echo '\nDay' $day && yarn --silent start $day; fi done"
  },
  "author": "David Margolin",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4"
  },
  "license": "Apache-2.0"
}